{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "resources/js/**/*",
        "resources/js/**/*.vue",
    ],
    "exclude": [
        "resources/js/Tests/*",
    ],
    "compilerOptions": {
        "composite": true,
        "allowJs": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "resources/js/*"
            ]
        }
    }
}
